package tugas5.no2;

import java.util.Scanner;

public class Tugas5No2 {
    
    public static class CekKubus {

        public static void kubus(int sisi1, int sisi2, int sisi3) {
            if (sisi1 == sisi2 && sisi2 == sisi3) {
                System.out.println("Termasuk Kubus");
            } else {
                System.out.println("Bukan termasuk Kubus");
            }
        }
    }
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("Masukkan sisi ke-1: ");
        int sisi1 = input.nextInt();

        System.out.print("Masukkan sisi ke-2: ");
        int sisi2 = input.nextInt();

        System.out.print("Masukkan sisi ke-3: ");
        int sisi3 = input.nextInt();

        CekKubus.kubus(sisi1, sisi2, sisi3);
       
    }
}
